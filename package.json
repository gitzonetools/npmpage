{
  "name": "npmpage",
  "version": "1.0.0",
  "description": "create pages for npm modules.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "(npm run compile && npm run clean && npm run setupCheck && npm run testCli && npm run npmpage)",
    "compile": "(npmts --notest)",
    "clean": "(rm -rf test/)",
    "testCli": "(cd test && npm install && npmts)",
    "npmpage": "(cd test && node ../dist/index.js)",
    "setupCheck": "(git clone https://gitlab.com/sandboxzone/sandbox-npmts.git test/)"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/pushrocks/npmpage.git"
  },
  "keywords": [
    "page",
    "generator"
  ],
  "author": "Lossless GmbH",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/pushrocks/npmpage/issues"
  },
  "homepage": "https://gitlab.com/pushrocks/npmpage#README",
  "dependencies": {
    "@types/q": "0.x.x",
    "beautylog": "^5.0.20",
    "early": "^2.0.26",
    "q": "^1.4.1",
    "smartcli": "^1.0.4",
    "smartfile": "^4.0.13",
    "smartpug": "^1.0.0",
    "typings-global": "^1.0.6"
  },
  "devDependencies": {}
}
